{
  "name": "insertafter",
  "version": "0.1.0",
  "private": true,
  "description": "Nicolas Froidure's homepage",
  "metapak": {
    "data": {
      "files": "'src/**/*.js' gulpfile.js 'gulp/**/*.js'"
    },
    "configs": [
      "www",
      "main",
      "eslint"
    ],
    "ignore": [
      "www"
    ]
  },
  "scripts": {
    "build": "gulp --prod && gulp publish --prod",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "dev": "gulp",
    "lint": "eslint 'src/**/*.js' gulpfile.js 'gulp/**/*.js'",
    "lint:html": "htmlhint 'contents/**/*.html'",
    "lint:urls": "reachable-urls 'contents/**/*.md' 'contents/**/*.html'",
    "metapak": "metapak",
    "precz": "npm t && npm run lint && npm run metapak -- -s",
    "prettier": "prettier --write 'src/**/*.js' gulpfile.js 'gulp/**/*.js'",
    "preversion": "npm t && npm run lint && npm run metapak -- -s",
    "test": "gulp build --prod --noreq --nolr --nobro --nowatch --nosrv",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "homepage": "https://insertafter.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/blog"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/blog/issues"
  },
  "author": {
    "name": "Nicolas Froidure",
    "email": "nicolas.froidure@insertafter.com",
    "url": "http://insertafter.com/en/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "asttpl": "^1.0.0",
    "browserify": "^16.2.3",
    "buildbranch": "^2.0.1",
    "combine-stream": "0.0.4",
    "commitizen": "^3.0.4",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "express": "^4.16.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-clone": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-cond": "^1.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-jshint": "^2.1.0",
    "gulp-less": "^4.0.1",
    "gulp-livereload": "^4.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-marked": "^1.0.0",
    "gulp-mdvars": "^2.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.4.0",
    "gulp-rework": "^4.0.0",
    "gulp-spawn": "^0.4.4",
    "gulp-streamify": "^1.0.2",
    "gulp-svgmin": "^2.1.0",
    "gulp-util": "^3.0.8",
    "gulp-vartree": "^2.0.1",
    "gulp-watch": "^5.0.1",
    "html-to-text": "^4.0.0",
    "htmlhint": "^0.10.1",
    "internal-ip": "^3.0.1",
    "lunr": "^2.3.5",
    "metapak": "^2.0.0",
    "metapak-nfroidure": "9.0.3",
    "nunjucks": "^3.1.6",
    "open": "^6.0.0",
    "plexer": "^1.0.2",
    "prettier": "^1.14.3",
    "reachable-urls": "^1.0.2",
    "request": "^2.88.0",
    "rework-rem2px": "^0.0.1",
    "rimraf": "~2.6.2",
    "streamfilter": "^3.0.0",
    "varstream": "^0.3.2",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "moment": "^2.23.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.12.0"
  },
  "contributors": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "eslint",
      "eslint-config-prettier",
      "prettier"
    ]
  },
  "files": [
    "src/**/*.js",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "sourceType": "script",
      "modules": true
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "semi": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  }
}
